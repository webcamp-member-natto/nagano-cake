<table style="color:white;">
  <thead>
    <tr>
      <th colspan="1"></th>
      <th class="header1">ジャンル編集</th>
    </tr>
  </thead>
  <tbody>
    <%= form_with model: @genre, url:admin_genre_path(@genre.id), method: :patch do |f| %>
    <tr>
      <td>ジャンル名</td>
      <td><%= f.text_field :name %></td>
      <td><%= f.submit '変更を保存' %></td>
    </tr>
    <% end %>
  </tbody>
</table>


